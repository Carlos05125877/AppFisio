{
  "expo": {
    "name": "Corona Fatos",
    "slug": "Corona Fatos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoApp.png",
    "scheme": "com.projeto.coronafatos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.projeto.coronafatos"
    },
    "android": {
      "package": "com.projeto.coronafatos",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoApp.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoApp.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoApp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}